msgid "Findinfiles Plugin"
msgstr "Plugin Findinfiles"

#. the plugin name
#: src/findinfiles.c:39
msgid "Find matches in various files"
msgstr "busca coincidencias en varios archivos"

#: src/findinfiles.c:72 src/interface.c:202
msgid "Find in files"
msgstr "Buscar en archivos"

#: src/interface.c:83
msgid "Search term"
msgstr "Término de búsqueda"

#: src/interface.c:91
msgid "Depth"
msgstr "Profundidad"

#: src/interface.c:103
msgid "Click here to start a search"
msgstr "Pulse aqui para comenzar una búsqueda"

#: src/interface.c:115
msgid "Base directory"
msgstr "Directorio base"

#: src/interface.c:119
msgid "From document"
msgstr ""

#: src/interface.c:122
msgid "Click here to set the base directory from current's document one."
msgstr ""
"Pulse aqui para configurar el directorio base desde el directorio actual"

#: src/interface.c:127
msgid "Click here to set the base directory"
msgstr "Pulse aqui para configurar el directorio base"

#: src/interface.c:132
msgid "Enter a base directory to search"
msgstr "Introduce un directorio base para buscar"

#: src/interface.c:210
msgid "In current base directory"
msgstr "En el directorio actual"

#: src/interface.c:214
msgid "In file's base directory"
